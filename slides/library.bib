  address = {Cham},
  doi = {10.1007/978-3-031-43059-6_18},
  abstract = {The adequacy and efficacy of doctor--patient communication has remained contentious. It is a fact that some patients are seen seeking reprieve from social media. Therefore, this study aimed at finding out the function, semantic and communication manifestations of Facebook Hypertension Support Groups. This study is perhaps the first linguistic oriented one to be carried out in sub-Saharan Africa and reveals an alternative health crisis communication method in sub-Saharan Africa. The research was guided by four main objectives which are: (1) To analyse the language models used in doctor--patient discourse. (2) To explain the dialogic nature of messages shared in Facebook Hypertension Support Groups. (3) To examine the function and impact of Facebook Hypertension Support Groups messages (4) To evaluate the High Blood Pressure patients' benefits and challenges of Hypertension Support Groups' communication. The theories underpinning this narrative qualitative research were the Multimodal Discourse Analysis and Dialogism. Data was collected through content/linguistic analysis of sampled and selected texts from Facebook Hypertension Support Groups. Interviews were also used. Purposive sampling was done in the selection of patients. The study revealed that the reciprocal emotional support characteristic of the Facebook Hypertension Support group is beneficial to the patients. More importantly, it was ascertained that the power balance between doctors and patients becomes more equalised, resulting in increased quality of clinical decisions.},
  isbn = {978-3-031-43059-6}
}

@article{Nyhan2010,
  title = {When {{Corrections Fail}}: {{The Persistence}} of {{Political Misperceptions}}},
  author = {Nyhan, Brendan and Reifler, Jason},
  year = {2010},
  journal = {Political Behavior},
  doi = {10.1007/S11109-010-9112-2},
  abstract = {An extensive literature addresses citizen ignorance, but very little research focuses on misperceptions. Can these false or unsubstantiated beliefs about politics be corrected? Previous studies have not tested the efficacy of corrections in a realistic format. We conducted four experiments in which subjects read mock news articles that included either a misleading claim from a politician, or a misleading claim and a correction. Results indicate that corrections frequently fail to reduce misperceptions among the targeted ideological group. We also document several instances of a ``backfire effect'' in which corrections actually increase misperceptions among the group in question.}
}

