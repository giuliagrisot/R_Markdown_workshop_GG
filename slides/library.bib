@article{Dyka2023,
  title = {{The Impact of Digitalization of Education on the Development of Key Teacher Competencies}},
  author = {Dyka, Natalia and Tretiak, Olha and Horobets, Svitlana and Yakunin, Yaroslav and Shopina, Maryna and Tsybulska, Svitlana},
