@incollection{Tally2015,
  title = {Introduction: {{The World}}, the {{Text}}, and the {{Geocritic}}},
  booktitle = {The {{Geocritical Legacies}} of {{Edward W}}. {{Said}}: {{Spatiality}}, {{Critical Humanism}}, and {{Comparative Literature}}},
