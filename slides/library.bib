@article{Oltean1993,
  title = {A Survey of the Pragmatic and Referential Functions of Free Indirect Discourse},
  author = {Oltean, Stefan},
  year = {1993},
  journal = {Poetics Today},
  volume = {14},
  number = {4},
  pages = {691--714},
  publisher = {{[Duke University Press, Porter Institute for Poetics and Semiotics]}},
  issn = {03335372, 15275507},
  doi = {10.2307/3301100},
  abstract = {This article proposes an interpretation of the pragmatic and referential functions of free indirect discourse, a discourse mode used especially in literary narrative. The issue of what constitutes free indirect discourse is discussed, the relevance of syntactic, semantic, and pragmatic accounts is assessed, and a three-tiered interpretation of its functions (including the integrative, evaluative, and referential functions) is proposed. The integrative and evaluative functions emphasize free indirect discourse as a bivocal or polyvocal mode, while the referential functions are seen to be relevant because they enable a more satisfactory conceptualization of free indirect discourse as the representation of verbal acts and preverbal or nonverbal mental acts. Free indirect discourse is therefore viewed not as report but as representation, a broader category which subsumes not only the rendition of verbal events, but nonverbal mental events as well.}
}

