@article{Ma2021a,
  title = {What Is a {{Good Visualization}} for {{Digital Humanities Researchers}}? {{An Exploratory Study}}},
  author = {Ma, Rongqian and Xiao, Fanghui},
  year = {2021},
  month = oct,
  journal = {Proceedings of the Association for Information Science and Technology},
  volume = {58},
  number = {1},
