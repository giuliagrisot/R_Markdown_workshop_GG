@article{Earley-Spadoni2020,
  title = {Spatial {{Archaeology}}: {{Mapping}} the {{Ancient Past}} with the {{Humanities}} and the {{Sciences}}},
  shorttitle = {Spatial {{Archaeology}}},
  author = {{Earley-Spadoni}, Tiffany and Harrower, Michael J.},
  year = {2020},
