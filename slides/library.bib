  keywords = {computational linguistics,Computational linguistics,document structure analysis,edge weights model,Electronic mail,emotional relationships,entertainment,natural language understanding,network theory (graphs),play emotional development,Pragmatics,Psychology,sentiment analysis,sentiment movement,sentiment network,sentiment network extraction,sentiment networks,Shakespeare plays,social network,Social network services,Speech,structured dialogue,Syntactics,text analysis}
