@article{Grimaud1984,
  title = {Poetics from Psychoanalysis to Cognitive Psychology},
  author = {Grimaud, Michel},
  year = {1984},
  journal = {Poetics},
  volume = {13},
  number = {4-5},
  pages = {325--345},
  issn = {0304422X},
  doi = {10.1016/0304-422X(84)90011-1},
  abstract = {Psychopoetics is, by definition, a multi-disciplinary venture: first in its need to consider the various subfields and schools of psychology and semiotics - including the study of literary discourses; second in its need to make use of a variety of methodologies in order to deal effectively with those diverse goals and disciplines. The present overview relates psychoanalysis - historically, the predominant psychological approach of literary scholars - to poetics (or literary semiotics) and suggests that psychoanalysts and poeticians, because of a natural distrust for the anti-mentalistic bias of behaviorist psychology, have been show in taking full advantage of the pro-mentalistic, process-oriented views of cognitive scientists in cognitive psychology and artificial intelligence. The first part of this overview, 'Freudianism versus Psychoanalysis: A Historical Survey', is divided into seven sections: Beyond Freud; Ego psychology; Object relations theory; Self psyvhology; The impact of Jacques Lacan; Psychoanalysis and contemporary psychology; Stages of Cognitive and psychosexual development in psychology and psychoanalysis. The second part on 'Psychology and Poetics' considers two classic alternatives: the choice between a structural and a functional view of literature and the status of the text between author and reader. The last two sections consider the contributions of experimental poetics, German-language poetics, and artificial intelligence to a psychologically based theory of texts. {\copyright} 1984.},
  isbn = {0304-422X}
}

