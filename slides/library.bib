@article{Goodlad2020,
  title = {A {{Study}} in {{Distant Reading}}: {{Genre}} and the {{{\emph{Longue Dur{\'e}e}}}} in the {{Age}} of {{AI}}},
  shorttitle = {A {{Study}} in {{Distant Reading}}},
  author = {Goodlad, Lauren M. E.},
