@article{Tilton2023,
  title = {Relating to {{Historical Sources}}},
  author = {Tilton, Lauren},
  year = {2023},
  month = sep,
  journal = {The American Historical Review},
  volume = {128},
  number = {3},
  pages = {1354--1359},
  issn = {0002-8762},
  doi = {10.1093/ahr/rhad365},
  urldate = {2024-04-22},
  abstract = {Artificial intelligence is everywhere, including seemingly adjacent to most historical research. As a stand-in for a plethora of methods, concepts, and theories, including machine learning (ML), natural language processing, and computer vision, the boundaries of artificial intelligence are so unclear and porous that they are hard to discern and even define. Historians have also been cautious to embrace the term, shaped by its association with the computational (social) sciences, important critiques of AI, and heated debates over computation in the humanities. For those who seek to signal the importance of these technologies to their practice, we often couch computational methods that fall under AI, such as text analysis and image analysis, under terms like ``digital humanities'' or ``digital history.'' Reading with and against the grain, identifying patterns and outliers, and toggling between close and distant reading are becoming analytical moves facilitated by artificial intelligence that are helping to rewrite how we understand the past.1 Ambitious projects such as the UK-based Living with Machines are also modeling how interdisciplinary teams can forge new AI approaches while writing new histories. It can be tempting to think that one isn't using or affected by artificial intelligence if they aren't using a tool like Mallet or programming with Python. Yet, arguably the most transformational and pervasive change for the field is AI's role in shaping how we organize and obtain historical sources, which is built around text search and browse.}
}

