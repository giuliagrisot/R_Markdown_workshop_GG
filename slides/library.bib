@incollection{Perfetti1999,
  title = {Cognitive Research and the Misconception of Reading},
  booktitle = {Reading Development and the Teaching of Reading: {{A}} Psychological Perspective},
  author = {Perfetti, Charles A},
  year = {1999},
  abstract = {There are obstacles to the use of good research in any field, and in reading education the obstacles are compounded. For one thing, research on reading abounds, promiscuously popping up in disciplines of all sorts and in publications of widely varying quality. Second, the problems of practice are more complex than the conclusions that come from research. However there are 4 clear contributions of cognitive research: (1) contrary to the belief that context allows readers to just sample a small of words in a text, skilled reader read most words rather than skip the words; (2) contrary to the belief that poor reading arises from a failure to use context, less skilled readers rely heavily on context; (3) contrary to a belief that the sounds of words (their phonology) are irrelevant once children learn to read, skilled readers use phonology in reading; (4) contrary to the view that learning to read is only or mainly about "meaning making," children learn to read successfully by learning how their writing system works.},
