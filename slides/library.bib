@article{Balint2016,
  title = {Reconceptualizing Foregrounding. {{Identifying}} Response Strategies to Deviation in Absorbing Narratives},
  author = {B{\'a}lint, Katalin and Hakemulder, Frank and Kuijpers, Moniek and Doicaru, Miruna M and Tan, Ed S},
  year = {2016},
  journal = {Scientific Study of Literature},
  volume = {6},
  number = {2},
  pages = {176--207},
  publisher = {John Benjamins},
  doi = {10.1075/ssol.6.2.02bal},
  abstract = {The experience of deviation is often referred to as foregrounding and contrasted with the experience of feeling absorbed in a narrative. However, instead of simply assuming that foregrounding and absorption are mutually exclusive, they should also be considered as co-occurring: being absorbed as a result of a deviating aspect of a story. In the present paper we examine the co-occurrence of responses by means of a data-driven qualitative approach. The analysis of interviews about absorbed experiences with written and cinematic fictional narratives focused on occurrences of and responses to perceived deviation. We identified seven strategies in response to deviation that may be described through three underlying dimensions: absorption, agency, and valence. Findings suggest that perceived deviation, rather than obstructing absorption, is associated with intense and meaningful engagement with narratives.}
}

