@incollection{Lunt2024,
  title = {Introduction -- {{Audience Theories}} and {{Approaches}}},
  booktitle = {The {{Routledge Companion}} to {{Media Audiences}}},
  author = {Lunt, Peter},
  year = {2024},
  publisher = {Routledge},
  abstract = {Submitting `audience theory' into a search engine results in a familiar list of theories that originated in the era of mass communication, such as effects research, uses and gratifications theory, active audience theory, reception theory combined with cultural studies, and cultivation theory. However, these are not audience theories as such but represent the focus on audiences from various approaches to understanding media, which were, in turn, derived from a variety of academic disciplines, including literary, cultural, social, sociological, psychological, and rhetorical theories. Although these approaches all used the concept of `audiences', they are widely varied and mainly had one thing in common: being attracted to the analysis of the rapidly emerging media technology of the second half of the 20th century: television. Television was a phenomenon that was both powerful---technologically, socially, and culturally---and diffused so rapidly that it attracted, perhaps demanded, the attention of theorists from across the social sciences, arts, and humanities, all of which were interested in how people responded to, what they did with, and how they were affected by media. However, the commonality across these approaches could have been more transparent, and the discontinuities were not so much at the level of observation and analysis but inherent in the assumptions from the different disciplinary and normative approaches.},
  isbn = {978-1-003-26854-3}
}

